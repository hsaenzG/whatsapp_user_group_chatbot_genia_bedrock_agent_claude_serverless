{
	"openapi": "3.0.0",
	"info": {
		"title": "AWS User Group LATAM API",
		"description": "Exposes the events for agent",
		"version": "1.0.0"
	},
	"paths": {
		"/event": {
			"get": {
				"description": "Get all the events host by the AWS User Group LATAM",
				"responses": {
					"200": {
						"description": "All the events hosted by the AWS User Group LATAM",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"id_event": {
											"type": "integer"
										},
										"Call_for_Speakers": {
											"type": "string"
										},
										"fecha": {
											"type": "string",
											"format": "date"
										},
										"hora_fin": {
											"type": "string",
											"format": "time"
										},
										"hora_inicio": {
											"type": "string",
											"format": "time"
										},
										"name": {
											"type": "string"
										},
										"registro": {
											"type": "string"
										},
										"sede": {
											"type": "string"
										},
										"sitioweb": {
											"type": "string"
										},
										"Sponsor": {
											"type": "string"
										},
										"status": {
											"type": "integer"
										},
										"ubicacion": {
											"type": "string"
										},
										"sesionize-api": {
											"type": "string"
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}