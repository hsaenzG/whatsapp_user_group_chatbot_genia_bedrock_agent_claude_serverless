{
	"openapi": "3.0.0",
	"info": {
	  "title": "Event API",
	  "version": "1.0.0"
	},
	"paths": {
	  "/event": {
		"get": {
		  "summary": "Obtener información sobre un evento",
		  "responses": {
			"200": {
			  "description": "Información del evento",
			  "content": {
				"application/json": {
				  "schema": {
					"type": "object",
					"properties": {
					  "id_event": {
						"type": "integer",
						"example": 1
					  },
					  "Call_for_Speakers": {
						"type": "string",
						"example": "https://sessionize.com/aws-community-day-guatemala-2024/"
					  },
					  "fecha": {
						"type": "string",
						"format": "date",
						"example": "19/10/2024"
					  },
					  "hora_fin": {
						"type": "string",
						"format": "time",
						"example": "17:00:00"
					  },
					  "hora_inicio": {
						"type": "string",
						"format": "time",
						"example": "09:00:00"
					  },
					  "name": {
						"type": "string",
						"example": "AWS Community Day - Guatemala 2024"
					  },
					  "registro": {
						"type": "string",
						"example": "https://www.eventbrite.com/e/aws-community-day-guatemala-2024-tickets-956193639917?aff=oddtdtcreator"
					  },
					  "sede": {
						"type": "string",
						"example": "Campus Central Universidad Rafael Landivar"
					  },
					  "sitioweb": {
						"type": "string",
						"example": "https://awscommunitygt.com/"
					  },
					  "Sponsor": {
						"type": "string",
						"example": "quieres ser sponsor ve acá: https://wa.me/50256536538"
					  },
					  "status": {
						"type": "integer",
						"example": 1
					  },
					  "ubicacion": {
						"type": "string",
						"example": "https://maps.app.goo.gl/Hppjr4t929HGJwKF9"
					  },
					  "sesionize-api": {
						"type": "string",
						"example": "https://sessionize.com/api/v2/your-sessionize-api-url"
					  }
					}
				  }
				}
			  }
			}
		  }
		}
	  }
	}
  }
  