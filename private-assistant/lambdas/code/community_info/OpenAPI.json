{
	"openapi": "3.0.0",
	"info": {
		"title": "AWS User Group Guatemala API",
		"description": "Exposes the events for agent",
		"version": "1.0.0"
	},
	"paths": {
		"/community": {
			"get": {
				"description": "Get the AWS User Group information",
				"responses": {
					"200": {
						"description": "Return all the information of the AWS User Group Guatemala, social media links, chapters and ways to connect",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"descripcion": {
											"type": "string"
										},
										"facebook": {
											"type": "string",
											"format": "url"
										},
										"girlschapter": {
											"type": "string"
										},
										"instagram": {
											"type": "string",
											"format": "url"
										},
										"linkedin": {
											"type": "string",
											"format": "url"
										},
										"meetup": {
											"type": "string",
											"format": "url"
										},
										"whatsapp": {
											"type": "string",
											"format": "url"
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}